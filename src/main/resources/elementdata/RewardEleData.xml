<?xml version="1.0" encoding="UTF-8"?>
<!--发放奖励页面元素-->
<RewardEleData>
    <!--赛区下拉框-->
    <property name="zone_button" type="xpath" value="//input[@name='areas']"/>
    <!--五服测试服-->
    <property name="test_server" type="xpath" value="//li[text()='5 - test server']"/>
    <!--角色输入框-->
    <property name="role_input" type="xpath" value="//input[@name='uuid']"/>
    <!--角色下拉框-->
    <property name="role_buttonn" type="xpath" value="/html/body/div[4]/div[2]/div/div/div/div/div[1]/div[3]/div/div/div[2]/div/div/div[2]"/>
    <!--在线角色选项-->
    <property name="arole" type="xpath" value="//li[text()='在线角色']"/>
    <!--发奖原因下拉框-->
    <property name="reason_button" type="xpath" value="/html/body/div[4]/div[2]/div/div/div/div/div[1]/div[3]/div/div/div[3]/div/div/div[2]"/>
    <!--系统维护选项-->
    <property name="reason_wei" type="xpath" value="//li[text()='系统维护/更新补偿']"/>
    <!--详细说明输入框-->
    <property name="instructions" type="xpath" value="//textarea[@name='desc']"/>
    <!--添加物品按钮-->
    <property name="additems" type="id" value="button-1065-btnIconEl"/>
    <!--道具搜索框-->
    <property name="prop_input" type="xpath" value="//input[starts-with(@id, 'textfield-')][1]"/>
    <!--第一条道具选项-->
    <property name="first_prop" type="xpath" value="//table[starts-with(@id, 'gridview-')][1]"/>
    <!--物品数量-->
    <property name="prop_num" type="xpath" value="//input[starts-with(@id,'numberfield-')]"/>
    <!--确定按钮-->
    <property name="accept" type="xpath" value="//span[starts-with(@id,'button-')  and text()='确定']"/>
    <!--执行操作按钮-->
    <property name="perform" type="xpath" value="//span[starts-with(@id,'button-')  and text()='执行']"/>
    <!--确定执行按钮-->
    <property name="accept_perform" type="xpath" value="//span[starts-with(@id,'button-')  and text()='Yes']"/>
</RewardEleData>